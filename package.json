{
  "name": "base-react",
  "version": "0.0.1",
  "description": "Starter template for Vite and React (typescript), support tailwindcss, jest, cypress, eslint, prettier, husky and many more configured and ready to use",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aquil3sVoy/react-base"
  },
  "author": "Oscar Corona <oabc4004@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aquil3sVoy/react-base/issues"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "jest",
    "cypress",
    "eslint",
    "prettier",
    "vite",
    "redux",
    "husky"
  ],
  "scripts": {
    "start": "vite",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prettierrc": "prettier --write src",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^2.0.4",
    "@reduxjs/toolkit": "^1.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not IE 11"
  ],
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.8.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^28.0.8",
